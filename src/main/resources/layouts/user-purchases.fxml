<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="pane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.apdb4j.view.UserPurchasesController">
   <children>
      <Label alignment="CENTER" text="My purchases">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <VBox.margin>
            <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="ticketsButton" mnemonicParsing="false" onAction="#onTicketsButtonPressed" prefWidth="90.0" text="Tickets">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="seasonTicketsButton" mnemonicParsing="false" onAction="#onSeasonTicketsButtonPressed" text="Season tickets">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonPressed">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
   <padding>
      <Insets bottom="150.0" left="150.0" right="150.0" top="150.0" />
   </padding>
</VBox>
